{
  "Source": "D:\\Repos\\C#\\fdis\\samples",
  "Threads": 1,
  "Provider": {
    "Type": "FileReader",
    "Options": {
    }
  },
  "Consumers": [
    {
      "Type": "FileWriter",
      "Options": {
        "SaveFolder": "D:\\Repos\\C#\\fdis\\output",
        "BufferSize": 81920
      }
    }
