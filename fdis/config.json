    },
    {
      "Type": "FileArchiver",
      "Options": {
        "Regex": ".*\\.(mov|mp4|mkv|webm|avi)$",
        "ArchiveName": "vods.zip"
      }
    },
    {
      "Type": "FileArchiver",
      "Options": {
        "Regex": "^(?!.*\\.zip$).*$",
        "ArchiveName": "rest.zip"
      }
    }
  ]
}
